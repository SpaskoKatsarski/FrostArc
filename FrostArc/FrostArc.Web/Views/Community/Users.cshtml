@model CommunityUsersViewModel

<h2>@Model.Name's Members</h2>

@if (Model.Users.Any())
{
    <div class="community-members">
        @foreach (var member in Model.Users)
        {
            <div class="member">
                <h3>@member.DisplayName</h3>
                <img src="@member.ProfilePictureUrl" alt="Profile Picture" class="profile-picture" />
            </div>
            <a asp-controller="Community" asp-action="RemoveUser" asp-route-communityId=""></a>
        }
    </div>
}
else
{
    <p>No members found in the community.</p>
}